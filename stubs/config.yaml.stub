# URL of the mantis system, e.g. https://tickets.company.tld
MANTIS_URL: {{mantisUrl}}
# user token as created within mantis
MANTIS_TOKEN: {{mantisToken}}

# github personal access token
GITHUB_TOKEN: {{githubToken}}
# target repo where issues will be created, e.g. user/reponame
GITHUB_REPOSITORY: {{githubRepository}}
